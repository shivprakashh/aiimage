}
.convert{
    width:6%;
    height:auto;
    border-radius: 50%;
    background-color: yellow;
    box-shadow:  0px 0px 10px 2px rgb(206, 206, 206);
    animation: conv 2s infinite ease-in
    ;
}
@keyframes conv {
    0%{transform: rotateZ(0deg);}
   50%{transform: scale(1.2) rotateZ(180deg);}
   100%{transform: rotateZ(360deg);}
    
}
.upload-con{
   
    padding:3px;
