}
.plus{
   box-shadow: 0px 0px 10px 1px rgb(188, 188, 188);
 width:30px;
 display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 40px;

 height:30px;
 text-transform: uppercase;
border-radius: 50%;
box-sizing: border-box;
  font-size: 20px;
    cursor: pointer;
  
   
    color:rgb(0, 0, 0);
    animation: pop 1s infinite ease-out;
 
}
@keyframes pop {
    0%{transform: scale(1);}
    50%{transform: scale(1.2);}
    100%{transform: scale(1);}
}
.limg{
    width:100px;
    position: absolute;
    left:10px;
    top:20px;
    border-radius: 10px;
    height:auto;
}
.par{
 background-color: white;
    padding:5px 10px;
  
