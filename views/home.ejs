}
.bt1-r-img1{
width:80%;
height:auto;
border-radius: 9px;
object-fit: cover;
}
.bt1-r-img2{
    position: absolute;
    width:50%;
    height:auto;
    border-radius: 12px;
    top:50px;
    left:-20px;
    animation: img 2s infinite ease-out;
}
@keyframes img {
    0%{transform: translateY(0px);}
    50%{transform: translateY(200px);}
    100%{transform: translateY(0px);}
   
}
@media only screen and (max-width: 700px) {
  .par {
    width:97%;
